{
  "name": "my-nuxt-starter",
  "version": "1.0.0",
  "description": "Yizack nuxt template",
  "author": "Yizack Rangel",
  "type": "module",
  "homepage": "https://github.com/Yizack/my-nuxt-starter",
  "repository": {
    "type": "git",
    "url": "https://github.com/Yizack/my-nuxt-starter"
  },
  "bugs": {
    "url": "https://github.com/Yizack/my-nuxt-starter/issues"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev --port=5173 --host",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "pnpm lint:eslint --fix && pnpm lint:stylelint --fix",
    "lint:eslint": "eslint --config .config/eslint.mjs",
    "lint:stylelint": "stylelint \"**/*.{css,scss,sass}\" --config .config/stylelint.json",
    "lint:eslint:inspect": "npx @eslint/config-inspector --config .config/eslint.mjs",
    "test:types": "vue-tsc --noEmit"
  },
  "devDependencies": {
    "@nuxt/eslint": "^0.3.13",
    "@nuxt/icon": "^1.3.1",
    "@nuxtjs/color-mode": "^3.4.2",
    "@nuxtjs/sitemap": "^5.3.5",
    "@types/bootstrap": "^5.2.10",
    "bootstrap": "^5.3.3",
    "eslint": "^9.8.0",
    "nuxt": "^3.12.4",
    "sass": "^1.77.8",
    "stylelint": "^16.7.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "typescript": "^5.5.4",
    "vue-tsc": "^2.0.29"
  },
  "packageManager": "pnpm@9.6.0"
}
