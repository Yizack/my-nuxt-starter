{
  "private": true,
  "name": "my-nuxt-starter",
  "version": "1.0.0",
  "description": "Yizack nuxt template",
  "type": "module",
  "license": "MIT",
  "author": {
    "name": "Yizack Rangel",
    "email": "yizackr@gmail.com",
    "url": "https://yizack.com"
  },
  "homepage": "https://github.com/Yizack/my-nuxt-starter",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yizack/my-nuxt-starter.git"
  },
  "bugs": "https://github.com/Yizack/my-nuxt-starter/issues",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev --port=5173 --host",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "analyze": "nuxt analyze",
    "lint": "pnpm lint:eslint && pnpm lint:stylelint",
    "lint:fix": "pnpm lint:eslint --fix && pnpm lint:stylelint --fix",
    "lint:eslint": "eslint --config .config/eslint.mjs",
    "lint:eslint:inspect": "pnpm dlx @eslint/config-inspector --config .config/eslint.mjs",
    "lint:stylelint": "stylelint \"**/*.{css,scss,sass}\" --config .config/stylelint.json",
    "test:types": "vue-tsc --noEmit"
  },
  "devDependencies": {
    "@nuxt/eslint": "^1.3.1",
    "@nuxt/icon": "^1.12.0",
    "@nuxtjs/color-mode": "^3.5.2",
    "@nuxtjs/sitemap": "^7.2.10",
    "@types/bootstrap": "^5.2.10",
    "bootstrap": "^5.3.6",
    "eslint": "^9.26.0",
    "nuxt": "^3.17.3",
    "sass": "^1.88.0",
    "stylelint": "^16.19.1",
    "stylelint-config-standard-scss": "^15.0.0",
    "typescript": "^5.8.3",
    "vue-tsc": "^2.2.10"
  },
  "packageManager": "pnpm@10.11.0"
}
